{% extends '@Protected/shells/secure.html.twig' %}
{% block main %}
<form method="post">
	{% if error %}<div class="errors">{{error.messageKey|trans(error.messageData, 'security')}}</div>{% endif %}
	<input name="_csrf_token" type="hidden" value="{{csrf_token('authenticate')}}" />
	<label>
		<div>Username</div>
		<input name="_username" value="{{lastUser}}" />
	</label>
	<label>
		<div>Password</div>
		<input name="_password" type="password" value="{{lastUser}}" />
	</label>
	<button type="submit">Log in</button>
</form>
{% endblock %}
